# RunPod API Configuration
RUNPOD_ENDPOINT_ID=your_endpoint_id
RUNPOD_API_KEY=your_api_key

# Local development API URLs
VITE_INGEST_API_URL=http://localhost:3000
VITE_EMBEDDING_API_URL=http://localhost:8000
VITE_ASSEMBLY_API_URL=http://localhost:3001

# R2 (Cloudflare) Storage Configuration
# docpacks go in doctown-central/docpacks/[repo_owner]/[repo_name].docpack on output
BUCKET_NAME=your-bucket-name
BUCKET_ACCESS_KEY_ID=your_access_key_id
BUCKET_SECRET_ACCESS_KEY=your_secret_access_key
BUCKET_S3_ENDPOINT=https://your-account-id.r2.cloudflarestorage.com
